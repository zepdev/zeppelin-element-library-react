{
  "name": "@zlab-de/zel-react",
  "version": "1.0.5",
  "private": false,
  "author": "Alyssa Butler for Zeppelin GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/zepdev/zeppelin-element-library-react"
  },
  "description": "React Components for Zeppelin Element Library",
  "keywords": [
    "react",
    "react-component",
    "zeppelin",
    "ZLab"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm && yarn build:es && yarn build:umd",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --config-file ../../babel.config.js ./src --out-dir ./build --ignore \"**/*.test.js\"",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel --config-file ../../babel.config.js ./src --out-dir ./build/esm --ignore \"**/*.test.js\"",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ../../babel.config.js ./src --out-dir ./build/es --ignore \"**/*.test.js\"",
    "build:umd": "cross-env BABEL_ENV=production-umd rollup -c scripts/rollup.config.js"
  },
  "peerDependencies": {
    "@types/react": "^16.8.6",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "clsx": "^1.0.2",
    "downshift": "^5.0.0-alpha.0",
    "prop-types": "^15.7.2",
    "zeppelin-element-library": "^1.3.0"
  },
  "devDependencies": {
    "react-test-renderer": "^16.11.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "./build/index.js",
  "module": ".build/esm/index.js",
  "browser": ".build/esm/index.js",
  "unpkg": "build/umd/zel-react.production.min.js",
  "files": [
    "build/*",
    "assets/*",
    "guidelines.json",
    "zel.css"
  ]
}
